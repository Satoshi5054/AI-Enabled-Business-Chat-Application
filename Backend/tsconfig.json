{
  "compilerOptions": {
    "target": "ES2020",

    "module": "NodeNext",
    "moduleResolution": "NodeNext",

    "rootDir": ".",
    "outDir": "dist",

    "types": ["node"],
    "typeRoots": ["./node_modules/@types"],

    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src","prisma","*.ts"],
  "exclude": ["node_modules", "dist"]
}
